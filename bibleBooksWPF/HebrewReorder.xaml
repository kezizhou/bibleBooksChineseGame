            <Label x:Name="lbl28" Grid.Column="2" Grid.Row="1" Padding="5" Background="#F0F3F5" Margin="10,5,10,10"  Style="{StaticResource RoundedCornerLabel}"/>
            <Label x:Name="lbl29" Grid.Column="2" Grid.Row="2" Padding="5" Background="#F0F3F5" Margin="10,5,10,10"  Style="{StaticResource RoundedCornerLabel}"/>
            <Label x:Name="lbl30" Grid.Column="2" Grid.Row="3" Padding="5" Background="#F0F3F5" Margin="10,5,10,10"  Style="{StaticResource RoundedCornerLabel}"/>
            <Label x:Name="lbl31" Grid.Column="2" Grid.Row="4" Padding="5" Background="#F0F3F5" Margin="10,5,10,10"  Style="{StaticResource RoundedCornerLabel}"/>
